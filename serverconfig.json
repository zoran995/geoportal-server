{
  "$schema": "./server-config.schema.json",
  "port": "${port:-3002}",
  "initPaths": [
    "init/",
    "init2/",
    "init/init3"
  ],
  "proxy": {
    "allowProxyFor": [
      "127.0.0.1",
      "127.0.0.1:3002",
      "127.0.0.1:3003",
      "docs.nestjs.com",
      "ci.terria.io",
      "nationalmap.gov.au",
      "services.ga.gov.au"
    ],
    "proxyAuth": {
      "127.0.0.1:3002": {
        "authorization": ""
      }
    }
  },
  "feedback": {
    "primaryId": "g1",
    "options": [
      {
        "service": "github",
        "id": "g1",
        "accessToken": "${accessToken}",
        "issuesUrl": "${issuesUrl}"
      }
    ]
  }
}