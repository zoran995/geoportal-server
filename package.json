{
  "name": "geoportal-server",
  "version": "0.0.1",
  "description": "A rewrite of TerriJS-Server using NestJS",
  "author": "Zoran Kokeza",
  "private": false,
  "license": "MIT",
  "scripts": {
    "prebuild": "npx rimraf dist",
    "build": "nest build",
    "format-check": "prettier --check \"src/**/*.ts\" \"test/**/*.ts\"",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start --",
    "start:pm2": "pm2 start ecosystem.config.js --update-env --",
    "stop:pm2": "pm2 stop ecosystem.config.js",
    "start:dev": "nest start --watch --",
    "start:debug": "nest start --debug --watch --",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix --max-warnings 0",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:e2e:cov": "jest --coverage  --config ./test/jest-e2e.json",
    "tool:merge-coverage": "ts-node --pretty --project=tools/tsconfig.tools.json tools/merge-coverage.ts",
    "prepare": "yarn run build",
    "preversion": "yarn run format && yarn run lint && yarn run build"
  },
  "main": "./geoportal-server.js",
  "files": [
    "dist",
    "geoportal-server.js"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/zoran995/geoportal-server.git"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.45.0",
    "@aws-sdk/node-http-handler": "^3.40.0",
    "@nestjs/axios": "0.0.5",
    "@nestjs/common": "^8.2.4",
    "@nestjs/config": "1.2.0",
    "@nestjs/core": "^8.2.4",
    "@nestjs/platform-express": "^8.2.4",
    "@nestjs/serve-static": "^2.2.2",
    "@nestjs/swagger": "^5.1.5",
    "axios": "git://github.com/zoran995/axios.git#geoportal",
    "base-x": "^3.0.9",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.1",
    "compression": "^1.7.4",
    "helmet": "^4.6.0",
    "nodemailer": "^6.7.2",
    "proj4-cli-defs": "^1.0.0",
    "range_check": "^2.0.4",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.5.1",
    "swagger-ui-express": "^4.3.0",
    "yargs": "^17.3.1"
  },
  "devDependencies": {
    "@golevelup/ts-jest": "0.3.2",
    "@nestjs/cli": "8.2.1",
    "@nestjs/schematics": "8.0.6",
    "@nestjs/testing": "8.3.1",
    "@types/compression": "1.7.2",
    "@types/express": "4.17.13",
    "@types/istanbul-lib-report": "3.0.0",
    "@types/jest": "27.4.0",
    "@types/node": "17.0.5",
    "@types/nodemailer": "6.4.4",
    "@types/supertest": "2.0.11",
    "@types/validator": "13.7.1",
    "@types/yargs": "17.0.8",
    "@typescript-eslint/eslint-plugin": "5.12.0",
    "@typescript-eslint/parser": "5.12.0",
    "aws-sdk-client-mock": "0.6.0",
    "eslint": "8.9.0",
    "eslint-config-prettier": "8.4.0",
    "eslint-plugin-prettier": "4.0.0",
    "istanbul-lib-coverage": "3.2.0",
    "istanbul-lib-report": "3.0.0",
    "jest": "27.5.1",
    "memfs": "3.4.1",
    "prettier": "2.5.1",
    "supertest": "6.2.2",
    "ts-jest": "27.1.3",
    "ts-loader": "9.2.6",
    "ts-node": "10.5.0",
    "tsconfig-paths": "3.12.0",
    "typescript": "4.5.5"
  },
  "resolutions": {
    "axios": "git://github.com/zoran995/axios.git#geoportal"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageReporters": [
      "json",
      "lcov"
    ],
    "collectCoverageFrom": [
      "src/**/*.(t|j)s",
      "!src/main.ts"
    ],
    "coverageDirectory": "coverage/unit",
    "coveragePathIgnorePatterns": [
      ".module.ts$",
      ".spec.ts$"
    ],
    "testEnvironment": "node",
    "modulePaths": [
      "<rootDir>"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/dist/"
    ]
  }
}