{
  "name": "@zoran995/geoportal-server",
  "version": "0.0.1-alpha",
  "description": "A rewrite of TerriJS-Server using NestJS",
  "author": "Zoran Kokeza",
  "license": "MIT",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com",
    "@zoran995:registry": "https://npm.pkg.github.com"
  },
  "scripts": {
    "prebuild": "npx rimraf dist",
    "build": "nest build",
    "format-check": "prettier --check \"src/**/*.ts\" \"test/**/*.ts\"",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start --",
    "start:dev": "nest start --watch --",
    "start:debug": "nest start --debug --watch --",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test,tools}/**/*.ts\" --fix --max-warnings 0",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:e2e:cov": "jest --coverage  --config ./test/jest-e2e.json",
    "tool:generate-schema": "ts-node --pretty --project=tools/tsconfig.tools.json -r tsconfig-paths/register tools/schema.ts",
    "tool:merge-coverage": "ts-node --pretty --project=tools/tsconfig.tools.json tools/merge-coverage.ts",
    "prepare": "yarn run build",
    "preversion": "yarn run format && yarn run lint && yarn run build"
  },
  "bin": "geoportal-server.js",
  "main": "./geoportal-server.js",
  "files": [
    "dist",
    "geoportal-server.js"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/zoran995/geoportal-server.git"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "3.699.0",
    "@nestjs/axios": "3.1.2",
    "@nestjs/common": "10.4.8",
    "@nestjs/config": "3.3.0",
    "@nestjs/core": "10.4.8",
    "@nestjs/platform-express": "10.4.8",
    "@nestjs/serve-static": "4.0.2",
    "@nestjs/swagger": "8.0.7",
    "@smithy/node-http-handler": "3.3.1",
    "axios": "github:zoran995/axios.git#geoportal",
    "base-x": "5.0.0",
    "compression": "1.7.5",
    "dotenv": "16.4.5",
    "dotenv-expand": "12.0.1",
    "helmet": "8.0.0",
    "nestjs-zod": "^4.2.0",
    "nodemailer": "6.9.16",
    "proj4-cli-defs": "1.0.0",
    "range_check": "2.0.4",
    "reflect-metadata": "0.2.2",
    "rxjs": "7.8.1",
    "sanitize-filename": "1.6.3",
    "yargs": "17.7.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@golevelup/ts-jest": "0.6.1",
    "@nestjs/cli": "10.4.8",
    "@nestjs/schematics": "10.2.3",
    "@nestjs/testing": "10.4.8",
    "@testcontainers/localstack": "^10.15.0",
    "@types/compression": "1.7.5",
    "@types/express": "5.0.0",
    "@types/istanbul-lib-report": "3.0.3",
    "@types/jest": "29.5.14",
    "@types/lodash": "^4.17.13",
    "@types/node": "22.9.3",
    "@types/nodemailer": "6.4.17",
    "@types/supertest": "6.0.2",
    "@types/yargs": "17.0.33",
    "@typescript-eslint/eslint-plugin": "5.56.0",
    "@typescript-eslint/parser": "5.56.0",
    "aws-sdk-client-mock": "4.1.0",
    "eslint": "8.36.0",
    "eslint-config-prettier": "8.8.0",
    "istanbul-lib-coverage": "3.2.2",
    "istanbul-lib-report": "3.0.1",
    "jest": "29.7.0",
    "memfs": "4.14.0",
    "msw": "^2.6.6",
    "prettier": "3.3.3",
    "supertest": "7.0.0",
    "testcontainers": "^10.15.0",
    "ts-jest": "29.2.5",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "4.9.5",
    "zod-to-json-schema": "^3.23.5"
  },
  "resolutions": {
    "axios": "github:zoran995/axios.git#geoportal"
  }
}
