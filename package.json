{
  "name": "geoportal-server",
  "version": "0.0.1",
  "description": "A rewrite of TerriJS-Server using NestJS",
  "author": "Zoran Kokeza",
  "private": false,
  "license": "MIT",
  "scripts": {
    "prebuild": "npx rimraf dist",
    "build": "nest build",
    "format-check": "prettier --check \"src/**/*.ts\" \"test/**/*.ts\"",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start --",
    "start:pm2": "pm2 start ecosystem.config.js --update-env --",
    "stop:pm2": "pm2 stop ecosystem.config.js",
    "start:dev": "nest start --watch --",
    "start:debug": "nest start --debug --watch --",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test,tools}/**/*.ts\" --fix --max-warnings 0",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:e2e:cov": "jest --coverage  --config ./test/jest-e2e.json",
    "tool:generate-schema": "ts-node --pretty --project=tools/tsconfig.tools.json -r tsconfig-paths/register tools/schema.ts",
    "tool:merge-coverage": "ts-node --pretty --project=tools/tsconfig.tools.json tools/merge-coverage.ts",
    "prepare": "yarn run build",
    "preversion": "yarn run format && yarn run lint && yarn run build"
  },
  "main": "./geoportal-server.js",
  "files": [
    "dist",
    "geoportal-server.js"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/zoran995/geoportal-server.git"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "3.299.0",
    "@aws-sdk/node-http-handler": "3.296.0",
    "@nestjs/axios": "2.0.0",
    "@nestjs/common": "9.3.12",
    "@nestjs/config": "3.0.0",
    "@nestjs/core": "9.3.12",
    "@nestjs/platform-express": "9.3.12",
    "@nestjs/serve-static": "3.0.1",
    "@nestjs/swagger": "6.2.1",
    "axios": "github:zoran995/axios.git#826b003",
    "base-x": "4.0.0",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.0",
    "class-validator-jsonschema": "5.0.0",
    "compression": "1.7.4",
    "dotenv": "16.0.3",
    "dotenv-expand": "9.0.0",
    "helmet": "6.0.1",
    "nodemailer": "6.9.1",
    "proj4-cli-defs": "1.0.0",
    "range_check": "2.0.4",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.8.0",
    "sanitize-filename": "1.6.3",
    "swagger-ui-express": "4.6.2",
    "yargs": "17.7.1"
  },
  "devDependencies": {
    "@golevelup/ts-jest": "0.3.5",
    "@nestjs/cli": "9.3.0",
    "@nestjs/schematics": "9.0.4",
    "@nestjs/testing": "9.3.12",
    "@types/compression": "1.7.2",
    "@types/express": "4.17.17",
    "@types/istanbul-lib-report": "3.0.0",
    "@types/jest": "29.5.0",
    "@types/node": "18.15.10",
    "@types/nodemailer": "6.4.7",
    "@types/supertest": "2.0.12",
    "@types/validator": "13.7.14",
    "@types/yargs": "17.0.23",
    "@typescript-eslint/eslint-plugin": "5.56.0",
    "@typescript-eslint/parser": "5.56.0",
    "aws-sdk-client-mock": "2.1.1",
    "eslint": "8.36.0",
    "eslint-config-prettier": "8.8.0",
    "istanbul-lib-coverage": "3.2.0",
    "istanbul-lib-report": "3.0.0",
    "jest": "29.5.0",
    "memfs": "3.4.13",
    "prettier": "2.8.7",
    "supertest": "6.3.3",
    "ts-jest": "29.0.5",
    "ts-loader": "9.4.2",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.1.2",
    "typescript": "4.9.5"
  },
  "resolutions": {
    "axios": "github:zoran995/axios.git#826b003"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageReporters": [
      "json",
      "lcov"
    ],
    "collectCoverageFrom": [
      "src/**/*.(t|j)s",
      "!src/main.ts"
    ],
    "coverageDirectory": "coverage/unit",
    "coveragePathIgnorePatterns": [
      ".module.ts$",
      ".spec.ts$",
      "index.ts"
    ],
    "testEnvironment": "node",
    "modulePaths": [
      "<rootDir>"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/dist/"
    ]
  }
}
