{
  "name": "@zoran995/geoportal-server",
  "version": "0.0.1-alpha",
  "description": "A rewrite of TerriJS-Server using NestJS",
  "author": "Zoran Kokeza",
  "license": "MIT",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com",
    "@zoran995:registry": "https://npm.pkg.github.com"
  },
  "scripts": {
    "prebuild": "npx rimraf dist",
    "build": "nest build",
    "format-check": "prettier --check \"src/**/*.ts\" \"test/**/*.ts\"",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start --",
    "start:dev": "nest start --watch --",
    "start:debug": "nest start --debug --watch --",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test,tools}/**/*.ts\" --fix --max-warnings 0",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.ts",
    "test:e2e:cov": "jest --coverage  --config ./test/jest-e2e.ts",
    "tool:generate-schema": "ts-node --pretty --project=tools/tsconfig.tools.json -r tsconfig-paths/register tools/schema.ts",
    "tool:merge-coverage": "ts-node --pretty --project=tools/tsconfig.tools.json tools/merge-coverage.ts",
    "prepare": "yarn run build",
    "preversion": "yarn run format && yarn run lint && yarn run build"
  },
  "bin": "geoportal-server.js",
  "main": "./geoportal-server.js",
  "files": [
    "dist",
    "geoportal-server.js"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/zoran995/geoportal-server.git"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "3.802.0",
    "@nestjs/axios": "4.0.0",
    "@nestjs/common": "11.1.0",
    "@nestjs/config": "4.0.2",
    "@nestjs/core": "11.1.0",
    "@nestjs/platform-express": "11.1.0",
    "@nestjs/serve-static": "5.0.3",
    "@nestjs/swagger": "11.1.6",
    "@smithy/node-http-handler": "4.0.4",
    "axios": "github:zoran995/axios.git#geoportal",
    "base-x": "5.0.1",
    "compression": "1.8.0",
    "dotenv": "16.5.0",
    "dotenv-expand": "12.0.2",
    "helmet": "8.1.0",
    "nestjs-zod": "4.3.1",
    "nodemailer": "7.0.0",
    "proj4-cli-defs": "1.0.0",
    "range_check": "2.0.4",
    "rate-limiter-flexible": "5.0.5",
    "reflect-metadata": "0.2.2",
    "rxjs": "7.8.2",
    "sanitize-filename": "1.6.3",
    "yargs": "17.7.2",
    "zod": "3.24.3"
  },
  "devDependencies": {
    "@golevelup/ts-jest": "0.6.2",
    "@nestjs/cli": "11.0.7",
    "@nestjs/schematics": "11.0.5",
    "@nestjs/testing": "11.1.0",
    "@testcontainers/localstack": "10.25.0",
    "@types/compression": "1.7.5",
    "@types/express": "5.0.1",
    "@types/istanbul-lib-report": "3.0.3",
    "@types/jest": "29.5.14",
    "@types/lodash": "^4.17.16",
    "@types/node": "22.15.3",
    "@types/nodemailer": "6.4.17",
    "@types/supertest": "6.0.3",
    "@types/yargs": "17.0.33",
    "@typescript-eslint/eslint-plugin": "5.56.0",
    "@typescript-eslint/parser": "5.56.0",
    "aws-sdk-client-mock": "4.1.0",
    "eslint": "8.36.0",
    "eslint-config-prettier": "8.8.0",
    "istanbul-lib-coverage": "3.2.2",
    "istanbul-lib-report": "3.0.1",
    "jest": "29.7.0",
    "memfs": "4.17.1",
    "msw": "2.7.5",
    "prettier": "3.3.3",
    "supertest": "7.1.0",
    "testcontainers": "10.25.0",
    "ts-jest": "29.3.2",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.8.3",
    "unionfs": "4.5.4",
    "zod-to-json-schema": "3.24.5"
  },
  "resolutions": {
    "axios": "github:zoran995/axios.git#geoportal"
  }
}
